@prefix adjoin:  <http://id.unit4.com/2011/11/01/adjoin-ns#> .

@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> . 
@prefix void: <http://rdfs.org/ns/void#> .

<http://id.unit4.com/template/AdJoin>
	a void:Dataset ;
	rdfs:comment "Template."@en ;
	void:subset <http://id.unit4.com/template/AdJoin/Default> ;
.

<http://id.unit4.com/template/AdJoin/Default>
	a void:Dataset ;
	rdfs:comment "Default template for use with AdJoin."@en ;
.

<http://id.unit4.com/template/AdJoin/Default/Header>
	rdfs:comment "Default header."@en ;
	adjoin:priority "1024"^^xsd:integer;

	adjoin:value [
		a rdf:Seq ;
		rdf:_1 [
			adjoin:value "$Common.setValue(\"randomUUID\", $UUID.randomUUID().toString())"^^xsd:string ;
		] ;
		rdf:_2 [
			adjoin:value "$Common.setValue(\"baseURI\", $String.format(\"http://id.example.org/%s\", $Common.getValue(\"randomUUID\")))"^^xsd:string ;
		] ;
		rdf:_3 [
			a rdf:Seq ;
			rdf:_1 [
				adjoin:value "$Common.setValue(\"examplePrefix\", \"exampleNS\")"^^xsd:string ;
			] ;
			rdf:_2 [
				adjoin:value "$Common.setValue(\"exampleNS\", $String.format(\"%s#\", $Common.getValue(\"baseURI\")))"^^xsd:string ;
			] ;
		] ;
		rdf:_4 [
			adjoin:value "$Output.getModel().setNsPrefix($Common.getValue(\"examplePrefix\"), $Common.getValue(\"exampleNS\"))"^^xsd:string ;		
		] ;
	] ;
	
	adjoin:statement [
		a rdf:Seq ;
		rdf:_1 [
			adjoin:subjectURI "http://id.example.org"^^xsd:String ;
			adjoin:propertyURI "$RDF.type" ;
			adjoin:objectType "$RDFS.Resource" ;
			adjoin:objectURI "$RDFS.Resource" ;
		] ;
		rdf:_2 [
			a rdf:Seq ;
			rdf:_1 [
				adjoin:subjectURI "http://id.example.org"^^xsd:String ;
				adjoin:propertyURI "$RDF.type" ;
				adjoin:objectType "$RDFS.Resource" ;
				adjoin:objectURI "$VoID.Dataset" ;
			] ;
			rdf:_2 [
				adjoin:triple [
					adjoin:subjectURI "http://id.example.org"^^xsd:String ;
					adjoin:propertyURI "$RDF.type" ;
					adjoin:objectType "$RDFS.Resource" ;
					adjoin:objectURI "$VoID.Linkset" ;
				] ;
			] ;
		] ;
	] ;
.

<http://id.unit4.com/template/AdJoin/Default/Header/CommonNSPrefixes>
	adjoin:triples [
		a rdfs:Seq ;
		rdf:_1 [
			adjoin:value "$Output.getModel().setNsPrefix(\"rdf\", $RDFClass.getURI())"^^xsd:string ;
		] ;
		rdf:_2 [
			adjoin:value "$Output.getModel().setNsPrefix($VoID.PREFIX, $VoID.NS)"^^xsd:string ;
		] ;
	] ;
.

<http://id.unit4.com/template/AdJoin/Default/Header/Dataset>
	rdfs:comment "Default header."@en ;
	adjoin:priority "1024"^^xsd:integer;
	adjoin:triples [
		a rdfs:Seq ;
		rdf:_1 [
			adjoin:subjectValue "$Common.getValue(\"baseURI\")" ;
			adjoin:propertyValue "$RDF.type" ;
			adjoin:objectType "$RDFS.Resource" ;
			adjoin:objectValue "$VoID.Dataset" ;
		] ;
		rdf:_2 [
			adjoin:value "$Common.setValue(\"baseURI\", $String.format(\"http://id.example.org/%s\", $Common.getValue(\"randomUUID\")))"^^xsd:string ;
		] ;
		rdf:_3 [
			adjoin:value "$Common.setValue(\"examplePrefix\", \"exampleNS\")"^^xsd:string ;
		] ;
		rdf:_4 [
			adjoin:value "$Common.setValue(\"exampleNS\", $String.format(\"%s#\", $Common.getValue(\"baseURI\")))"^^xsd:string ;
		] ;
		rdf:_5 [
			adjoin:value "$Output.getModel().setNsPrefix($Common.getValue(\"examplePrefix\"), $Common.getValue(\"exampleNS\"))"^^xsd:string ;		
		] ;
	] ;
.

<http://id.unit4.com/template/AdJoin/Default/Row>
	rdfs:comment "Default row regular expressions."@en;
	skos:narrower <http://id.unit4.com/template/AdJoin/Default/Row/Default> ;
.

<http://id.unit4.com/template/AdJoin/Default/Row/Default>
	rdfs:comment "Default i.e. match anything"@en ;
    adjoin:priority "1024"^^xsd:integer ;
	adjoin:pattern "^.*$" ;
	adjoin:triples [
		a rdfs:Seq ;
		rdf:_1 [
			adjoin:subjectValue "$Common.getValue(\"rowURI\")" ;
			adjoin:propertyValue "$String.format(\"%s%s\", $Common.getValue(\"exampleNS\"), $Columns.getName())" ;
			adjoin:objectType "$RDFS.Literal" ;
			adjoin:objectValue "$Row.getValue()" ;
			adjoin:objectDatatype "$XSD.xstring" ;
		] ;
	] ;
.

<http://id.unit4.com/template/AdJoin/Default/BeforeRow>
	adjoin:triples [
		a rdfs:Seq ;
		rdf:_1 [
			adjoin:value "$Common.setValue(\"rowURI\", $String.format(\"%s/%s\", $Common.getValue(\"baseURI\"), $Row.getIndex()))" ;
		] ;
	] ;
.
